@model IEnumerable<MvcAppTest2.Models.Speaker>

<script>
    $(function () {
        $("#testui").accordion();
    });
</script>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EmailAdd)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.BirthDay)
        </th>  
    </tr>
@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EmailAdd)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.BirthDay)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.SpeakerID }) |
            @Html.ActionLink("Details", "Details", new { id=item.SpeakerID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.SpeakerID })
        </td>
    </tr>
}

</table>


